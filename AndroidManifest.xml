<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alaa.amira.myrestaurant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Shared_Meals"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Individual_meals"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Extreme_Meals" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Family" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Economy_Meals" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Party" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Super_family" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>